

@model List<SQL_Mike_03_19_20.Models.testimonial>
@{
    ViewBag.Title = "saveGt";
    Layout = null;

}


@foreach (var item in Model)
{
    if (@item.Author != null && @item.Content != null)
    {
        <div class="col-md-6">


            <h3>Author: @item.Author</h3>
        </div>
        <div class="col-md-6">
            <h3>Content: @item.Content</h3>
        </div>
    }
}


